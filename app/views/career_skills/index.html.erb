<h1>Listing career_skills</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Career</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @career_skills.each do |career_skill| %>
      <tr>
        <td><%= career_skill.order %></td>
        <td><%= career_skill.career %></td>
        <td><%= career_skill.skill %></td>
        <td><%= link_to 'Show', career_skill %></td>
        <td><%= link_to 'Edit', edit_career_skill_path(career_skill) %></td>
        <td><%= link_to 'Destroy', career_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Career skill', new_career_skill_path %>
